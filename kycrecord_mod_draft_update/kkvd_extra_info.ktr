    <connection>KRA_v1_db</connection>
    <sql>SELECT 
    a.kmmt_instr_id,
    TRIM(b.kkvd_pan_no) AS trim_kkvd_pan_no,
    TRIM(b.kkvd_mi_id) AS trim_kkvd_mi_id,
    b.*,

    -- Constructing kkvd_extra_info JSONB object
    jsonb_set(
        jsonb_set(
            jsonb_build_object(
                'kkvd_sr_no', b.kkvd_sr_no,
                'kkvd_mi_id', b.kkvd_mi_id,
                'kkvd_sub_kua_code', b.kkvd_sub_kua_code,
                'kkvd_auth_type', b.kkvd_auth_type,
                'kkvd_kyc_type', b.kkvd_kyc_type,
                'kkvd_req_txn_ref', b.kkvd_req_txn_ref,
                'kkvd_mode_of_kyc', b.kkvd_mode_of_kyc,
                'kkvd_pan_no', b.kkvd_pan_no,
                'kkvd_aadhaar_seeding_sts', b.kkvd_aadhaar_seeding_sts,
                'kkvd_name_as_per_pan', b.kkvd_name_as_per_pan,
                'kkvd_mob_no', b.kkvd_mob_no,
                'kkvd_email_id', b.kkvd_email_id,
                'kkvd_txn_id', b.kkvd_txn_id,
                'kkvd_kra_ack_no', b.kkvd_kra_ack_no,
                'kkvd_res_time', b.kkvd_res_time,
                'kkvd_kra_sts_pending', b.kkvd_kra_sts_pending,
                'kkvd_kyc_ver_sts', b.kkvd_kyc_ver_sts,
                'kkvd_kyc_ver_sub_sts', b.kkvd_kyc_ver_sub_sts,
                'kkvd_kyc_name', b.kkvd_kyc_name,
                'kkvd_aadhar_holder_gender', b.kkvd_aadhar_holder_gender,
                'kkvd_aadhar_holder_dob', b.kkvd_aadhar_holder_dob,
                'kkvd_aadhar_holder_photo', b.kkvd_aadhar_holder_photo,
                'kkvd_add_line1', b.kkvd_add_line1,
                'kkvd_add_line2', b.kkvd_add_line2,
                'kkvd_add_line3', b.kkvd_add_line3,
                'kkvd_city', b.kkvd_city,
                'kkvd_pincode', b.kkvd_pincode,
                'kkvd_state', b.kkvd_state,
                'kkvd_crtd_by', b.kkvd_crtd_by,
                'kkvd_crtd_time', b.kkvd_crtd_time,
                'kkvd_updt_by', b.kkvd_updt_by,
