    ) AS kkvd_extra_info

FROM kra_mstr_modf_tbl AS a
JOIN kra_kyc_ver_dtls AS b 
    ON TRIM(a.kmmt_pan) = TRIM(b.kkvd_pan_no)
   AND TRIM(a.kmmt_bp_id) = TRIM(b.kkvd_mi_id)
   AND a.kmmt_ack_num = b.kkvd_kra_ack_no
